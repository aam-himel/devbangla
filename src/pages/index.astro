---
import Layout from '../layouts/Layout.astro'
import { Button } from '@/components/ui/button'
import { Card, CardContent, CardTitle } from '@/components/ui/card'
import { Input } from '@/components/ui/input'
import AccordionComp from '@/components/custom/accordion-comp'
import heroImg from '@/assets/img/hero-coding.svg'
import fire from '@/assets/img/fire.gif'
import { ResizableSection } from '@/components/custom/resizable-section'
---

<Layout title="Landing Page">
	<main class="mxw mx-auto h-auto sm:h-screen">
		<!-- hero section -->
		<div class="herobg overflow-hidden border-b dark:border-b">
			<svg
				class="animate-spotlight pointer-events-none absolute -top-20 left-0 z-[1] h-[150%] w-[120%] opacity-[.2] md:-top-10 md:left-40 lg:w-[80%]"
				xmlns="http://www.w3.org/2000/svg"
				viewBox="0 0 3787 2842"
				fill="none"
			>
				<g filter="url(#newFilter)">
					<ellipse
						cx="1824.71"
						cy="373.501"
						rx="1824.71"
						ry="373.501"
						transform="matrix(-0.822377 -0.568943 -0.568943 0.822377 3531.88 2191.09)"
						fill="white"
						fill-opacity="0.25"></ellipse>
				</g>
				<defs>
					<filter
						id="newFilter"
						x="0"
						y="0"
						width="3787"
						height="2842"
